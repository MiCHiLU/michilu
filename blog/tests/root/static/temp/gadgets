<script>

function confirmDelete(deleteURL) {
    var confirmed = confirm("このリストを削除してもいいですか？");
    if (confirmed) {
	document.location.href = deleteURL;
    }
}

function confirmUndelete(undeleteURL) {
    var confirmed = confirm("このリストを元に戻してもいいですか？");
    if (confirmed) {
	document.location.href = undeleteURL;
    }
}


</script>




















    





    
    





























